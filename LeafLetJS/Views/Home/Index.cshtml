@{
    ViewBag.Title = "Home Page";
}
<div id="map">

</div>
@section scripts
{
    <script type="text/javascript" src="https://webapi.amap.com/maps?v=1.3&key=cb156e4ebf0def93e395415dcb2024c1"></script>
    <script type="text/javascript">
        $(function() {
            // ReSharper disable once PossiblyUnassignedProperty
            var map = new window.AMap.Map('map',
                {
                    resizeEnable: true,
                    zoom: 16,
                    center: [120.106884, 30.217469]
                });
            abc();
        });

        function abc() {
            var map = L.map('map').setView([30, 118], 4);
            L.tileLayer('http://{s}.mapabc.com/mapabc/maptile?&x={x}&y={y}&z={z}',
                {
                    subdomains: ["emap1", "emap2", "emap3"]
                }).addTo(map);
            var marker = L.marker([30, 118]).addTo(map);
        }
    </script>
}